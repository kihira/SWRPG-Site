{
  "title": "SWRPG Site",
  "version": "0.1.0",
  "name": "swrpg-site",
  "author": {
    "name": "Kihira"
  },
  "private": true,
  "homepage": "https://swrpg.foxes.rocks",
  "scripts": {
    "build": "npm run less && npm run svg && npm run ts",
    "watch": "concurrently --kill-others \"tsc -p tsconfig.json --watch\" \"node watch.js\"",
    "less": "lessc ./assets/less/style.less ./static/style.css && lessc ./assets/less/edit.less ./static/edit.css",
    "svg": "svgo -f ./assets/svg -o ./static/img",
    "ts": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@types/chosen-js": "^1.6.2",
    "@types/datatables.net": "^1.10.9",
    "@types/datatables.net-fixedheader": "^3.1.3",
    "@types/datatables.net-rowgroup": "^1.0.0",
    "@types/jquery": "^3.3.1",
    "@types/jqueryui": "^1.12.2",
    "csso": "^3.5.0",
    "less": "^3.0.1",
    "svgo": "^1.0.5",
    "typescript": "^2.8.1",
    "uglify-js": "^3.3.21"
  },
  "devDependencies": {
    "@types/node": "^9.6.4",
    "@types/uglify-js": "^3.0.2",
    "concurrently": "^3.5.1",
    "node-watch": "^0.5.8",
    "tslint": "^5.9.1"
  }
}
