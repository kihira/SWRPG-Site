{
  "title": "SWRPG Site",
  "version": "0.1.0",
  "name": "swrpg-site",
  "author": {
    "name": "Kihira"
  },
  "private": true,
  "homepage": "https://kihira.uk",
  "scripts": {
    "build": "npm run less && npm run svg && npm run ts",
    "watch": "tsc --watch",
    "less": "lessc ./assets/style.less ./static/style.css",
    "svg": "svgo -f ./assets/svg -o ./static/img",
    "ts": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "less": "^3.0.1",
    "typescript": "^2.7.2",
    "svgo": "^1.0.5",
    "@types/datatables.net": "^1.10.8",
    "@types/datatables.net-rowgroup": "^1.0.0",
    "@types/datatables.net-fixedheader": "^3.1.3",
    "@types/jquery": "^3.3.1"
  },
  "devDependencies": {
    "tslint": "^5.9.1",
    "prettier": "^1.11.1"
  }
}
